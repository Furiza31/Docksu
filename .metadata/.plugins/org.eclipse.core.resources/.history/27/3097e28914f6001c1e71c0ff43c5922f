package application;

import java.io.File;
import java.util.Objects;

public class WorkspaceFile {
	
	File path;
	File parent;

	public WorkspaceFile (WorkspaceFile parent, String name) {
		if (Objects.isNull(parent)) {
			this.parent = null;
			this.path = new File(System.getProperty("user.dir") + "\\Workspace\\" + name);
		}
	}
	
}
